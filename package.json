{
  "name": "vr-fps-cafe-pwa",
  "version": "1.0.0",
  "description": "VR FPS Game Cafe Progressive Web App",
  "main": "src/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "serve dist",
