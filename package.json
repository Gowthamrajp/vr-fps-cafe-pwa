  },
  "devDependencies": {
    "@preact/preset-vite": "^2.7.0",
    "vite": "^5.0.10",
    "serve": "^14.2.1"
  },
  "keywords": [
    "vr",
    "fps",
    "game",
    "cafe",
    "pwa",
    "preact",
    "firebase"
